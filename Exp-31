#include <stdio.h>

int main() {
    int pages[] = {1, 3, 0, 3, 5, 6};
    int frame[3] = {-1, -1, -1}, k = 0;

    for (int i = 0; i < 6; i++) {
        frame[k] = pages[i];
        k = (k + 1) % 3;
        printf("Frames: ");
        for (int j = 0; j < 3; j++)
            printf("%d ", frame[j]);
        printf("\n");
    }
    return 0;
}
